@page "/OurStudent"
<!DOCTYPE html>
<html>
<body>
    <div> <h2>HTML Forms</h2></div>

    <div class="myform">
    <form action="#" id="formsave" method="get">
       
        <label for="fname">First Name:</label>
        <input type="text" id="fname" name="fname" placeholder="Enter your first Name"><br>
        <label for="lname">Last Name:</label>
        <input type="text" id="lname" name="lname" placeholder="Enter your first Name"><br>
        <label for="gender">Gender:</label>
        <select name="gender" id="gender">
            <option value="male">Male</option>
            <option value="female">Female</option>
            <option value="other">Other</option>
        </select> <br>
        <label for="birthday">Date of Birth</label>
        <input type="date" id="dateofbirth" name="dateofbirth">
        <label for="age">Age:</label>
        <input type="text" id="age" name="age"> <br>
        <label for="class">Class :</label>
        <input type="text" id="class" name="class" placeholder="Enter you Class"><br>
        <label for="address">Address :</label>
        <textarea id="address" name="address" placeholder="Enter you Address"></textarea>

        <br>
        <input type="submit" value="Submit" onclick="submitform">
 
    </form>
    </div>
   <script src="site.js"></script>

</body>
</html>



